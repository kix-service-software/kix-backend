{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "MarkObjectAsSeenPostRequest.json",
  "type": "object",
  "description": "The request object to mark an object as seen (if supported).",
  "properties": {
    "MarkObjectAsSeen": {
      "type": "object",
      "properties": {
        "IDs": {
          "type": "array",
          "items": {
            "anyOf": [
              { "type": "string" },
              { "type": "integer" }
            ]
          }
        },
        "ObjectType": {
          "type": "string"
        }
      },
      "required": [
        "IDs",
        "ObjectType"
      ]
    }
  },
  "required": [
    "MarkObjectAsSeen"
  ]
}