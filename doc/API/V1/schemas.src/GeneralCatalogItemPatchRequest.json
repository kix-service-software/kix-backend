{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "GeneralCatalogItemPatchRequest.json",
  "type": "object",
  "description": "The GeneralCatalog item request object for update.",
  "properties": {
    "GeneralCatalogItem": {
      "type": "object",
      "properties": {
        "Class": {
          "type": "string"
        },
        "Comment": {
          "type": "string"
        },
        "Name": {
          "type": "string"
        },
        "Preferences": {
          "type": "array",
          "items": {
            "$ref": "GeneralCatalogItemPreferencePostItem.json"
          }
        },
        "ValidID": {
          "type": "integer"
        }
      }
    }
  },
  "required": [
    "GeneralCatalogItem"
  ]
}