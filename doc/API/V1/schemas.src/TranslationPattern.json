{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "TranslationPattern.json",
  "type": "object",
  "description": "The translation pattern object.",
  "properties": {
    "AvailableLanguages": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "readOnly": true
    },
    "ChangeBy": {
      "type": "integer",
      "readOnly": true
    },
    "ChangeTime": {
      "type": "string",
      "readOnly": true
    },
    "CreateBy": {
      "type": "integer",
      "readOnly": true
    },
    "CreateTime": {
      "type": "string",
      "readOnly": true
    },
    "ID": {
      "type": "integer",
      "readOnly": true
    },
    "Value": {
      "type": "string",
      "readOnly": true
    },
    "Languages": {
      "type": "array",
      "description": "added via include \"Languages\"",
      "readOnly": true,
      "items": {
        "$ref": "TranslationLanguage.json"
      }
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "CreateBy",
    "CreateTime",
    "ID",
    "Value"
  ]
}