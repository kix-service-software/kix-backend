{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "OAuth2Profile.json",
  "type": "object",
  "description": "The oauth2 profile object.",
  "properties": {
    "ChangeBy": {
      "type": "integer",
      "readOnly": true
    },
    "ChangeTime": {
      "type": "string",
      "readOnly": true
    },
    "ClientID": {
      "type": "string",
      "readOnly": true,
      "description": "ID of the OAuth2 client."
    },
    "CreateBy": {
      "type": "integer",
      "readOnly": true
    },
    "CreateTime": {
      "type": "string",
      "readOnly": true
    },
    "HasAccessToken": {
      "type": "integer",
      "readOnly": true,
      "description": "Shows if profile has a valid access token (1 = yes, 0 = no)."
    },
    "ID": {
      "type": "integer",
      "readOnly": true
    },
    "Name": {
      "type": "string",
      "readOnly": true
    },
    "Scope": {
      "type": "string",
      "readOnly": true,
      "description": "Scope to request for the OAuth2 token."
    },
    "PKCE": {
      "type": "integer",
      "readOnly": true,
      "description": "Use PKCE for Authorization Code flow."
    },
    "URLAuth": {
      "type": "string",
      "readOnly": true,
      "description": "Base URL for the OAuth2 authorization."
    },
    "URLToken": {
      "type": "string",
      "readOnly": true,
      "description": "Base URL for requesting OAuth2 token."
    },
    "URLRedirect": {
      "type": "string",
      "readOnly": true,
      "description": "Base URL for redirects after OAuth2 authorization."
    },
    "ValidID": {
      "type": "integer",
      "readOnly": true
    }
  },
  "required": [
    "ChangeBy",
    "ChangeTime",
    "ClientID",
    "CreateBy",
    "CreateTime",
    "HasAccessToken",
    "ID",
    "Name",
    "Scope",
    "PKCE",
    "URLAuth",
    "URLToken",
    "URLRedirect",
    "ValidID"
  ]
}