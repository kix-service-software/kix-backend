         "type" : "object",
         "properties" : {
            "ChangeBy" : {
               "type" : "integer",
               "readOnly" : true
            },
            "ChangeTime" : {
               "type" : "string",
               "readOnly" : true
            },
            "Comment" : {
               "type" : "string",
               "readOnly" : true
            },
            "CreateBy" : {
               "type" : "integer",
               "readOnly" : true
            },
            "CreateTime" : {
               "type" : "string",
               "readOnly" : true
