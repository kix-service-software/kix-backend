      "_Macro_json" : {
         "$id" : "Macro.json",
         "$schema" : "http://json-schema.org/draft-07/schema",
         "description" : "The macro object.",
         "type" : "object",
         "properties" : {
            "Actions" : {
               "description" : "added via include \"Actions\"",
               "type" : "array",
               "item" : {
