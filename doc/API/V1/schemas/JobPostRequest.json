{
   "$id" : "JobPostRequest.json",
   "$schema" : "http://json-schema.org/draft-07/schema",
   "description" : "The job request object for creation.",
   "type" : "object",
   "properties" : {
      "Job" : {
         "type" : "object",
         "properties" : {
            "Comment" : {
               "type" : "string"
            },
            "ExecPlanIDs" : {
               "type" : "array",
               "item" : {
                  "type" : "integer"
               }
            },
            "Filter" : {
               "description" : "List of and/or search criteria (see chapter \"Search objects\" and \"Filter results\" for more details).",
               "type" : "array"
            },
            "IsAsynchronous" : {
               "type" : "integer"
            },
            "MacroIDs" : {
               "type" : "array",
               "item" : {
                  "type" : "integer"
               }
            },
            "Name" : {
               "type" : "string"
            },
            "SortOrder" : {
               "description" : "Used to sort found objects (see \"Filter\").",
               "type" : "object",
               "properties" : {
                  "Attribute" : {
                     "type" : "string"
                  },
                  "Descanding" : {
                     "type" : "integer"
                  }
               },
               "required" : [
                  "Attribute"
               ]
            },
            "Type" : {
               "description" : "",
               "type" : "string"
            },
            "ValidID" : {
               "type" : "integer"
            }
         },
         "required" : [
            "Name",
            "Type"
         ]
      }
   },
   "required" : [
      "Job"
   ]
}
