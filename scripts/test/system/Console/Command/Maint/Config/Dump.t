use strict;
use warnings;
use utf8;

use vars (qw($Self));

my $CommandObject = $Kernel::OM->Get('Console::Command::Maint::Config::Dump');

my ( $Result, $ExitCode );

{
    local *STDOUT;
    open STDOUT, '>:utf8', \$Result;    ## no critic
    $ExitCode = $CommandObject->Execute('Home');
}

$Self->Is(
    $ExitCode,
    0,
    "Exit code",
);

chomp $Result;

$Self->Is(
    $Result,
    $Kernel::OM->Get('Config')->Get('Home'),
    "Result",
);

1;



